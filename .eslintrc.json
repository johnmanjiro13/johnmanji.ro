{
  "settings": {
    "import/resolver":{
      "typescript": {}
    }
  },
  "plugins": ["import", "unused-imports"],
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "@typescript-eslint/no-unused-vars": 0,
    "import/order": [
      2,
      {
        "alphabetize": {
          "order": "asc"
        },
        "newlines-between": "always"
      }
    ],
    "unused-imports/no-unused-imports": 2,
    "no-restricted-imports": [
      2,
      {
        "patterns": ["../"]
      }
    ]
  }
}
